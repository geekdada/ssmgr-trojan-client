{
  "name": "@royli/ssmgr-trojan-client",
  "version": "1.1.4",
  "description": "ssmgr client for trojan-go",
  "bin": {
    "ssmgr-trojan-client": "./bin/ssmgr-trojan-client"
  },
  "keywords": [
    "ssmgr",
    "shadowsocks-manager",
    "trojan-go"
  ],
  "scripts": {
    "dev": "npm run clean && npm run ts:build -- --watch",
    "build": "run-s clean protos:build ts:build",
    "lint": "eslint src --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ts:build": "tsc",
    "protos:update": "zx ./scripts/update-protos.mjs",
    "protos:build": "zx ./scripts/build-protos.mjs",
    "send-test-payload": "zx ./scripts/send-test-payload.mjs",
    "install-trojan": "zx ./scripts/install-trojan.mjs",
    "clean": "rimraf dist",
    "prepare": "run-s protos:build",
    "prepublishOnly": "run-s build",
    "postinstall": "run-s install-trojan"
  },
  "author": "Roy Li<me@royli.dev>",
  "license": "MIT",
  "repository": "https://github.com/geekdada/ssmgr-trojan-client",
  "dependencies": {
    "@grpc/grpc-js": "^1.5.10",
    "@protobuf-ts/grpc-transport": "^2.4.0",
    "@protobuf-ts/plugin": "^2.4.0",
    "@protobuf-ts/runtime": "^2.4.0",
    "@protobuf-ts/runtime-rpc": "^2.4.0",
    "@sentry/node": "^6.19.3",
    "commander": "^7.2.0",
    "death": "^1.1.0",
    "execa": "^5.1.0",
    "fs-extra": "^10.1.0",
    "log4js": "^6.4.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sequelize": "^6.19.2",
    "sqlite3": "^5.0.8",
    "yaml": "^2.1.0",
    "zx": "^6.1.0"
  },
  "devDependencies": {
    "@types/death": "^1.1.2",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.1",
    "tslint": "^6.1.2",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16"
  }
}
